@model string


<!DOCTYPE HTML>
<html lang="ru">
	<head>
		<title>Dopetrope by HTML5 UP</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="/css/main.css"/>
	</head>
	<body class="homepage is-preload">
		<div id="page-wrapper">

				<!-- Header -->
				<section id="header">

					<!-- Logo -->
						<h1><a href="~/Home/Index/">Искусство черной магии</a></h1>

					<!-- Nav -->
						<nav id="nav">
							<ul>
								<li><a href="~/Home/Index/">Home</a></li>
															
								<li class="current"><a href="~/Ticket/Index/">Купить билет</a></li>
								<li ><a href="~/Home/Repertory/">Репертуар</a></li>
								
							</ul>
						</nav>

				</section>		
		</div>
<section id="main">
		@if (Model != "Превышен лимит участников." && Model != "Вы не достигли 18 лет." && Model != "Пользователь успешно удалён." && Model != "Неверные данные пользователя.")
{
    <h1 class="justify-content-center col-3 offset-5">Ваш уникальный код: @Model. <br/> Запишите его. С его помощью вы можете попасть на мероприятие или отменить запись на него.</h1>
}
else
{
    <h1 class="justify-content-center col-3 offset-5">@Model</h1>
}
</section>
<section id="footer">
					<div class="container">
						<div class="row">
							<div class="col-8 col-12-medium">
